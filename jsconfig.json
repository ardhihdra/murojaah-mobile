{
  "compilerOptions": {
      "target": "ES6",
      "moduleResolution": "node",
      "allowSyntheticDefaultImports": true,
      "baseUrl": "./",
      "paths": {
        "@api/*": ["./api/*"],
        "@components/*": ["./components/*"],
        "@constants/*": ["./constants/*"],
        "@styles/*": ["./styles/*"],
      }
  },
  "include": ["./**/*"],
  "exclude": ["node_modules", ".expo", ".vscode"]
}