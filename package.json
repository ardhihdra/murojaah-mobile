{
  "name": "murojaah-mobile",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --dev-client",
    "start-nocache": "expo start -c",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "fingerprint": "cd android && keytool -keystore app/debug.keystore -list -v",
    "prebuild-android": "expo prebuild --platform android",
    "build-local-native-android": "cd android && ./gradlew assembleRelease",
    "build-local-preview-android": "eas build -p android --profile preview --local",
    "build-local-production-android": "eas build -p android --profile production --local",
    "build-eas-preview-android": "eas build -p android --profile preview",
    "build-eas-production-android": "eas build -p android --profile production",
    "inspect--build-preview-android": "eas build:inspect --platform android --stage archive --output ./web-build/eas-inspects/android --profile preview"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "~1.17.3",
    "@react-native-firebase/app": "^17.1.0",
    "@react-native-firebase/auth": "^17.1.0",
    "@react-native-firebase/firestore": "^17.1.0",
    "@react-native-google-signin/google-signin": "^9.0.2",
    "@react-native-picker/picker": "2.4.8",
    "@react-navigation/bottom-tabs": "^6.4.3",
    "@react-navigation/native": "^6.0.16",
    "@react-navigation/native-stack": "^6.9.4",
    "@react-navigation/stack": "^6.3.7",
    "@rneui/base": "^0.0.0-edge.2",
    "@rneui/themed": "^0.0.0-edge.2",
    "expo": "~47.0.12",
    "expo-constants": "~14.0.2",
    "expo-dev-client": "~2.0.1",
    "expo-font": "~11.0.1",
    "expo-mail-composer": "~12.0.0",
    "expo-splash-screen": "~0.17.5",
    "expo-status-bar": "~1.4.2",
    "prebuild": "11.0.4",
    "react": "18.1.0",
    "react-native": "0.70.5",
    "react-native-chart-kit": "^6.12.0",
    "react-native-dotenv": "^3.4.6",
    "react-native-progress": "^5.0.0",
    "react-native-safe-area-context": "^4.4.1",
    "react-native-screens": "~3.18.0",
    "react-native-svg": "13.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9"
  },
  "private": true
}
